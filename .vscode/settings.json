{
    "files.associations": {
        "*.func": "shellscript"
    },
    "files.eol": "\n",
    "files.insertFinalNewline": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": "explicit"
    },
    "chat.tools.terminal.autoApprove": {
        "git commit": true,
        "git push": true
    }
}
